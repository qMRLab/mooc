,@ARTICLE{El-Sharkawy2006-hl,
  title    = "Monitoring and correcting spatio-temporal variations of the {MR}
              scanner's static magnetic field",
  author   = "El-Sharkawy, Abdel Monem and Schär, Michael and Bottomley, Paul A
